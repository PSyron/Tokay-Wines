-- --------------------------------------------------------

--
-- Struktura tabeli dla tabeli `tHotelImages`
--

CREATE TABLE IF NOT EXISTS `tHotelImages` (
`IdHotelImage` int(11) NOT NULL,
  `IdHotel_` int(11) NOT NULL,
  `IdImage_` int(11) NOT NULL,
  `LastUpdate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

-- --------------------------------------------------------

--
-- Struktura tabeli dla tabeli `tHotelReviews`
--

CREATE TABLE IF NOT EXISTS `tHotelReviews` (
`IdHotelReview` int(11) NOT NULL,
  `IdHotel_` int(11) NOT NULL,
  `Mark` double DEFAULT NULL,
  `IdDescription_` int(11) NOT NULL,
  `IdUser_` int(11) NOT NULL,
  `EntryDate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

-- --------------------------------------------------------

--
-- Struktura tabeli dla tabeli `tHotels`
--

CREATE TABLE IF NOT EXISTS `tHotels` (
`IdHotel` int(11) NOT NULL,
  `Email` varchar(50) NOT NULL,
  `Link` varchar(50) NOT NULL,
  `Name` varchar(20) NOT NULL,
  `Phone` varchar(20) DEFAULT NULL,
  `IdDescription_` int(11) NOT NULL,
  `IdAddress_` int(11) NOT NULL,
  `IdUser_` int(11) DEFAULT NULL,
  `IdImageCover_` int(11) NOT NULL,
  `LastUpdate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

