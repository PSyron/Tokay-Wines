--
-- Struktura tabeli dla tabeli `tFlavours`
--

CREATE TABLE IF NOT EXISTS `tFlavours` (
`IdFlavour` int(11) NOT NULL,
  `NameEng` varchar(30) NOT NULL,
  `NamePl` varchar(20) NOT NULL
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

