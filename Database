-- --------------------------------------------------------

--
-- Struktura tabeli dla tabeli `tWines`
--

CREATE TABLE IF NOT EXISTS `tWines` (
`IdWine` int(11) NOT NULL,
  `Name` varchar(40) NOT NULL,
  `ProdDate` int(4) NOT NULL,
  `Price` double(8,2) DEFAULT '0.00',
  `Volume` int(11) DEFAULT '0' COMMENT 'Volume of the bottle in cl. i.e.: 50 as for 50cl = 500ml',
  `AvailablePL` tinyint(1) NOT NULL DEFAULT '0',
  `Label` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Representative for the producer. 1 for yes 0 for no',
  `LastUpdate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `IdColor_` int(11) NOT NULL,
  `IdFlavour_` int(11) NOT NULL,
  `IdProducer_` int(11) NOT NULL,
  `IdDescription_` int(11) NOT NULL,
  `IdGrade_` int(11) DEFAULT NULL,
  `IdImageCover_` int(11) NOT NULL
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

