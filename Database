--
-- Procedury
--
CREATE DEFINER=`m1183_nietoperek`@`%.devil` PROCEDURE `pPlaces`()
    MODIFIES SQL DATA
BEGIN
DROP TABLE IF EXISTS tPlaces;
CREATE TABLE tPlaces (
IdPlace INT NOT NULL AUTO_INCREMENT,
Name VARCHAR(20) NOT NULL,
Address VARCHAR(70) NOT NULL,
Latitude DOUBLE NOT NULL,
Longitude DOUBLE NOT NULL,
PlaceType VARCHAR(20),
PRIMARY KEY(IdPlace)
);
  INSERT INTO tPlaces (Name, Address, Latitude, Longitude, PlaceType) 
(
SELECT tHotels.Name, CONCAT(tAddresses.StreetName,' ',tAddresses.StreetNumber,' ',tAddresses.City), tAddresses.Latitude, tAddresses.Longitude, "Hotel"
FROM tHotels
LEFT JOIN tAddresses
ON tHotels.IdAddress_=tAddresses.IdAddress
);


INSERT INTO tPlaces (Name, Address, Latitude, Longitude, PlaceType) 
(
SELECT tRestaurants.Name, CONCAT(tAddresses.StreetName,' ',tAddresses.StreetNumber,' ',tAddresses.City), tAddresses.Latitude, tAddresses.Longitude, "Restaurant"
FROM tRestaurants
LEFT JOIN tAddresses
ON tRestaurants.IdAddress_=tAddresses.IdAddress
);

INSERT INTO tPlaces (Name, Address, Latitude, Longitude, PlaceType) 
(
SELECT tMonuments.Name, CONCAT(tAddresses.StreetName,' ',tAddresses.StreetNumber,' ',tAddresses.City), tAddresses.Latitude, tAddresses.Longitude, "Monument"
FROM tMonuments
LEFT JOIN tAddresses
ON tMonuments.IdAddress_=tAddresses.IdAddress
);
INSERT INTO tPlaces (Name, Address, Latitude, Longitude, PlaceType) 
(
SELECT tProducers.Name, CONCAT(tAddresses.StreetName,' ',tAddresses.StreetNumber,' ',tAddresses.City), tAddresses.Latitude, tAddresses.Longitude, "Producer"
FROM tProducers
LEFT JOIN tAddresses
ON tProducers.IdAddress_=tAddresses.IdAddress
);
INSERT INTO tPlaces (Name, Address, Latitude, Longitude, PlaceType) 
(
SELECT tLookouts.Name, CONCAT(tAddresses.StreetName,' ',tAddresses.StreetNumber,' ',tAddresses.City), tAddresses.Latitude, tAddresses.Longitude, "Lookout"
FROM tLookouts
LEFT JOIN tAddresses
ON tLookouts.IdAddress_=tAddresses.IdAddress
);
INSERT INTO tPlaces (Name, Address, Latitude, Longitude, PlaceType) 
(
SELECT tOddities.Name, CONCAT(tAddresses.StreetName,' ',tAddresses.StreetNumber,' ',tAddresses.City), tAddresses.Latitude, tAddresses.Longitude, "Oddity"
FROM tOddities
LEFT JOIN tAddresses
ON tOddities.IdAddress_=tAddresses.IdAddress
);
INSERT INTO tPlaces (Name, Address, Latitude, Longitude, PlaceType) 
(
SELECT tNews.Header, CONCAT(tAddresses.StreetName,' ',tAddresses.StreetNumber,' ',tAddresses.City), tAddresses.Latitude, tAddresses.Longitude, "Event"
FROM tNews
JOIN tAddresses
ON tNews.IdAddress_=tAddresses.IdAddress
);
END$$

